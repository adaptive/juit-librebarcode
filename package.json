{
  "name": "@juit/librebarcode",
  "version": "1.0.0",
  "description": "Encoders and fonts for EAN13 and Code128",
  "author": "Team Juit <developers@juit.com>",
  "license": "Apache-2.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./assets/LibreBarcode128-Regular.ttf": {
      "asset": "./assets/LibreBarcode128-Regular.ttf"
    },
    "./assets/LibreBarcode128Text-Regular.ttf": {
      "asset": "./assets/LibreBarcode128Text-Regular.ttf"
    },
    "./assets/LibreBarcodeEAN13Text-Regular.ttf": {
      "asset": "./assets/LibreBarcodeEAN13Text-Regular.ttf"
    }
  },
 "scripts": {
    "build": "plug",
    "coverage": "plug coverage",
    "dev": "plug coverage -w src -w test",
    "lint": "plug lint",
    "test": "plug test",
    "transpile": "plug transpile"
  },
  "devDependencies": {
    "@plugjs/build": "^0.6.0"
  },
  "files": [
    "*.md",
    "assets/",
    "dist/",
    "src/"
  ]
}
